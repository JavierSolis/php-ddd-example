{
    "name": "javiersolis/phpddd",
    "description": "Ejemplo basico de DDD con php y doctrine",
    "type": "project",
    "license": "MIT",
    "autoload": {
        "psr-4": {
            "App\\": "./"
        }
    },
    "authors": [
        {
            "name": "Javier J. Solis Flores"
        }
    ],
    "minimum-stability": "stable",
    "require": {
        "ramsey/uuid": "^4.7",
        "doctrine/orm": "^3.3",
        "doctrine/dbal": "^4.2",
        "symfony/cache": "^7.2",
        "symfony/event-dispatcher": "^7.2",
        "symfony/http-foundation": "^7.2",
        "symfony/routing": "^7.2",
        "symfony/serializer": "^7.2",
        "symfony/validator": "^7.2",
        "symfony/yaml": "^7.2",
        "symfony/framework-bundle": "^7.2"
    },
    "config": {
        "bin-dir": "vendor/bin"
    },
    "require-dev": {
        "phpunit/phpunit": "^11.5",
        "symfony/browser-kit": "^7.2"
    }
}
