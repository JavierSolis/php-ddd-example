{
    "name": "javiersolis/phpddd",
    "description": "Ejemplo basico de DDD con php y doctrine",
    "type": "project",
    "license": "MIT",
    "autoload": {
        "psr-4": {
            "App\\": "./src/"
        }
    },
    "authors": [
        {
            "name": "Javier J. Solis Flores"
        }
    ],
    "minimum-stability": "stable",
    "require": {
        "ramsey/uuid": "^4.7",
        "doctrine/orm": "^3.3",
        "doctrine/dbal": "^4.2",
        "symfony/cache": "^7.2",
        "symfony/event-dispatcher": "^7.2",
        "slim/slim": "4.*",
        "nyholm/psr7": "^1.8",
        "nyholm/psr7-server": "^1.1"
    },
    "config": {
        "bin-dir": "vendor/bin"
    },
    "require-dev": {
        "phpunit/phpunit": "^11.5"
    }
}